          <a href="/index" class="logo">
            <img
              src="/images/logo.png"
              width="110"
              height="50"
              alt="EduWeb logo"
            />
          </a>

          <button
            class="nav-close-btn"
            aria-label="close menu"
            data-nav-toggler
