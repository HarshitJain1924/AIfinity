            <input type="submit" value="Environment" class="navbar-link" 
            >
            </li>
            </form>
            <li class="navbar-item">
            <a href="/resource" class="navbar-link" data-nav-link
              >Resources</a
            >
            </li>
            <li class="navbar-item">
            <a href="/payment" class="navbar-link" data-nav-link
              >Payment</a
            >
          </li>
        </ul>
      </nav>
      <div class="header-actions">
        <div class="img-name-container">
          <% if(locals.Name){ %>
          <img
            src="/images/user.png"
            class="user-pic"
            onclick="toggleMenu()"
          />
          <p class="img-container-text"><%= locals.Name %></p>
          <% }else{ %> <% } %>
